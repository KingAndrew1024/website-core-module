{
    "name": "@virket/website-core",
    "version": "1.0.7",
    "description": "Virket Website core module (interfaces, models, repositories, services, state management and more) for an ionic5 app",
    "main": "index.js",
    "typings": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "ngc": "ngc",
        "lint": "ng lint",
        "test": "ng test --no-watch",
        "build": "rm -rf aot dist && npm run ngc",
        "postinstall": "ngcc",
        "prePublishPackage": "npm run lint && npm run test && npm run build",
        "publishPackage": "npm run prePublishPackage && npm publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KingAndrew1024/website-core-module.git"
    },
    "keywords": [
        "ionic",
        "website",
        "angular9",
        "virket"
    ],
    "author": "Andres Vergara @ virket.com",
    "license": "MIT",
    "homepage": "https://github.com/KingAndrew1024/website-core-module#readme",
    "bugs": {
        "url": "https://github.com/KingAndrew1024/website-core-module/issues"
    },
    "dependencies": {
        "@ngrx/effects": "^9.2.0",
        "@ngrx/store": "^9.2.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.5",
        "@angular/cli": "~9.1.12",
        "@angular/common": "~9.1.6",
        "@angular/compiler": "~9.1.6",
        "@angular/compiler-cli": "~9.1.6",
        "@angular/core": "~9.1.6",
        "@angular/platform-browser": "~9.1.6",
        "@angular/platform-browser-dynamic": "~9.1.6",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "codelyzer": "^5.1.2",
        "@types/node": "^12.11.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "rxjs": "~6.5.1",
        "tslint": "^6.1.3",
        "typescript": "~3.8.3",
        "zone.js": "~0.10.2"
    }
}